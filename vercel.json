{
  "version": 2,
  "buildCommand": "pnpm nx build mob-api --prod",
  "outputDirectory": "dist/apps/mob-api",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": [
        "apps/mob-api/src/**",
        "libs/**"
      ]
    }
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    }
  ]
}

