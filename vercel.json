{
  "version": 2,
  "buildCommand": "pnpm nx build mob-api --prod && cp -r apps/mob-api/src dist/apps/mob-api/ && cp -r libs dist/apps/mob-api/",
  "outputDirectory": "dist/apps/mob-api",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "node_modules/swagger-ui-dist/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    }
  ]
}

